<%- if controller_name != 'sessions' %>
  <%= link_to t(".sign_in"), new_session_path(resource_name) %> or
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to omniauth_authorize_path(resource_name, provider) do %>
      <i class="fa fa-twitter blue-twitter"></i>
      <%= t('.sign_in_with_provider', provider: provider.to_s.titleize) %><br>
    <% end %>
  <% end -%>
<% end -%>
